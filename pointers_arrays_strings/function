#include "main.h"
#include <stdio.h>

/**
 * _strncpy - the other function to copy
 * @dest: the given funciton
 * @src: the given function
 * @n: the given function
 * Return: return the output 
 */

char *_strncpy(char *dest, char *src, int n)
{
	int i;

	for (i = 0; i < n && src[i] != '\0'; i++)
	{	
		dest[i] = src[i];
		dest[i] = '\0';
	}
	for (i = 0; i < n; i++)
	{
		dest[i] = '\0';
	}
	return (dest);	
}
